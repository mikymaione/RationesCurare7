                    <a id="a8" runat="server" href='mGrafico.aspx?T=Y'>
                        <img src="images/grafico55.png" title="Saldo" /></a>
                </asp:TableCell>
                <asp:TableCell>
                    <a id="a9" runat="server" href='mLogin.aspx?C=L'>
                        <img src="images/logout.png" title="Log-out" /></a>
                </asp:TableCell>
            </asp:TableRow>
        </asp:Table>
        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server" />
    </div>
